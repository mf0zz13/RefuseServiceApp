@page "/"


<MudDatePicker Label="Date" Editable="true" @bind-Date="DispatchDate" />
<MudSelect @bind-Value="ServiceArea" Label="Select Integer" Placeholder="Select Service Area">
    @foreach (int num in numArray)
    {
        <MudSelectItem Value="@num">@num</MudSelectItem>
    }
</MudSelect>
Number Selected is @ServiceArea


@code {
    private DateTime? DispatchDate { get; set; }
    private int ServiceArea { get; set; } 

    protected override void OnInitialized()
    {
        DispatchDate = DateTime.Today;
    }

    public int[] numArray = { 1, 2, 3, 4, 5 };
}